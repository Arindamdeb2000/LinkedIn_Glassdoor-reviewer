{
  "manifest_version": 2,

  "name": "GlassSort",
  "description": "This extension sorts companies by GlassDoor ratings",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/favicon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ]
}